{
  "name": "do-verse",
  "version": "0.0.1",
  "description": "Multiplayer worlds With Authentication Based on NFT ownership",
  "type": "module",
  "module": "./dist/index.mjs",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "scripts/postinstall.cjs",
    "dist/**/*",
    "lib/**/*",
    "env.d.ts",
    "src/**/*",
    "tsconfig.json"
  ],
  "author": "wighawag <wighawag@gmail.com>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conquest-eth/do-verse"
  },
  "volta": {
    "node": "16.13.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.13.0",
    "@ethersproject/abi": "^5.6.4",
    "@ethersproject/address": "^5.6.1",
    "@ethersproject/bytes": "^5.6.1",
    "@types/jest": "^28.1.2",
    "@types/node": "^18.0.0",
    "cross-env": "^7.0.3",
    "do-json-db": "0.0.17",
    "dotenv-cli": "^5.1.0",
    "esbuild": "^0.14.47",
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.7",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-miniflare": "^2.5.1",
    "miniflare": "^2.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.14"
  },
  "dependencies": {
    "@ethersproject/wallet": "^5.6.2",
    "do-ethereum-events": "0.2.13"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.cjs",
    "prepare": "node .setup.cjs",
    "build:js": "node build.js",
    "ts-run": "dotenv -- ts-node --experimental-specifier-resolution=node --esm",
    "build": "npm-run-all \"generate {1}\" \"build:js\" --",
    "format": "prettier --write '**/*.{js,ts,cjs,mjs,css,json,md}'",
    "generate": "node generate.cjs",
    "deploy:wrangler": "wrangler publish",
    "deploy": "npm-run-all \"generate {1}\" \"deploy:wrangler\" --",
    "dev:flare": "miniflare --kv ACCESS_TOKENS --live-reload --debug --modules dist/example.mjs",
    "dev": "npm-run-all \"generate {1}\" \"dev:flare\" --",
    "test": "npm run build && node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js",
    "types:check": "tsc && tsc -p test/tsconfig.json"
  },
  "typescriptLibraries": "lib"
}
